# nehme nur die mit zwei einträge (t0 und t1)
dat2 <- group_by(dat, Patientencode) %>% 
  summarise(occur = n()) %>%
  filter(occur == 2) %>%
  left_join(., data_wide, by = "Patientencode") # 46 personen

