	urldate = {2017-04-20},
	date = {2011-08-15},
	keywords = {Food, Meat, Dairy, Life cycle impacts, Environmentally extended input output analysis},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\egel\\switchdrive\\Zotero\\storage\\22WVDK6F\\Tukker et al. - 2011 - Environmental impacts of changes to healthier diet.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\egel\\switchdrive\\Zotero\\storage\\IMH853YC\\S092180091100190X.html:text/html}
